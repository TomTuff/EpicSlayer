type TMonster = record
  Name: TStringArray;
  SlayerLevel, Item: Integer;
  Drops: TIntegerArray;
end;


type TSlayerGear = record
  Equips: Boolean;
  Slot: Integer;
  Finisher: Boolean;
  MinFinishLP: Integer;
  ItemID: Integer;
end;


type TMaster = record
  Names: TStringArray;
  SlayerLevelReq, CombatLevelReq: Integer;
  Assigns: TIntegerArray;
end;
